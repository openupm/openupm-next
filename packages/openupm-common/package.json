    "rimraf": "~5.0",
    "ts-api-utils": "~1.0",
    "ts-jest": "~29.1",
    "typescript": "~5.1"
  },
  "scripts": {
    "start": "node build/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
